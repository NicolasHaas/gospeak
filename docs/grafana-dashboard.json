{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "type": "stat",
      "title": "Uptime",
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "gospeak_uptime_seconds",
          "legendFormat": "uptime",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Active Connections",
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 0 },
      "targets": [
        {
          "expr": "gospeak_connections_active",
          "legendFormat": "active",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 50 },
              { "color": "red", "value": 100 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Total Connections",
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 0 },
      "targets": [
        {
          "expr": "gospeak_connections_total",
          "legendFormat": "total",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": {}, "overrides": [] }
    },
    {
      "type": "stat",
      "title": "Auth Success",
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "gospeak_auth_success_total",
          "legendFormat": "success",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "color": { "mode": "fixed", "fixedColor": "green" } }, "overrides": [] }
    },
    {
      "type": "stat",
      "title": "Auth Failed",
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 0 },
      "targets": [
        {
          "expr": "gospeak_auth_failed_total",
          "legendFormat": "failed",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "color": { "mode": "fixed", "fixedColor": "red" } }, "overrides": [] }
    },
    {
      "type": "stat",
      "title": "Kicks / Bans",
      "gridPos": { "h": 4, "w": 4, "x": 20, "y": 0 },
      "targets": [
        {
          "expr": "gospeak_kicks_total",
          "legendFormat": "kicks",
          "refId": "A"
        },
        {
          "expr": "gospeak_bans_total",
          "legendFormat": "bans",
          "refId": "B"
        }
      ],
      "fieldConfig": { "defaults": {}, "overrides": [] }
    },
    {
      "type": "timeseries",
      "title": "Voice Packets / sec",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "targets": [
        {
          "expr": "rate(gospeak_voice_packets_in_total[1m])",
          "legendFormat": "in",
          "refId": "A"
        },
        {
          "expr": "rate(gospeak_voice_packets_out_total[1m])",
          "legendFormat": "out",
          "refId": "B"
        },
        {
          "expr": "rate(gospeak_voice_packets_dropped_total[1m])",
          "legendFormat": "dropped",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "pps",
          "custom": { "drawStyle": "line", "fillOpacity": 10 }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "dropped" },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "red" } }
            ]
          }
        ]
      }
    },
    {
      "type": "timeseries",
      "title": "Voice Bandwidth",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "targets": [
        {
          "expr": "rate(gospeak_voice_bytes_in_total[1m])",
          "legendFormat": "in",
          "refId": "A"
        },
        {
          "expr": "rate(gospeak_voice_bytes_out_total[1m])",
          "legendFormat": "out",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "Bps",
          "custom": { "drawStyle": "line", "fillOpacity": 10 }
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Connections Over Time",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "targets": [
        {
          "expr": "gospeak_connections_active",
          "legendFormat": "active",
          "refId": "A"
        },
        {
          "expr": "rate(gospeak_connections_total[5m])",
          "legendFormat": "new/s",
          "refId": "B"
        },
        {
          "expr": "rate(gospeak_disconnects_total[5m])",
          "legendFormat": "disconnects/s",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "line", "fillOpacity": 10 }
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Chat Messages / min",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "targets": [
        {
          "expr": "rate(gospeak_chat_messages_total[1m]) * 60",
          "legendFormat": "messages/min",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "bars", "fillOpacity": 50 }
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Channels Created",
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 20 },
      "targets": [
        {
          "expr": "gospeak_channels_created_total",
          "legendFormat": "created",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": {}, "overrides": [] }
    },
    {
      "type": "stat",
      "title": "Channels Deleted",
      "gridPos": { "h": 4, "w": 4, "x": 4, "y": 20 },
      "targets": [
        {
          "expr": "gospeak_channels_deleted_total",
          "legendFormat": "deleted",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": {}, "overrides": [] }
    },
    {
      "type": "stat",
      "title": "Tokens Created",
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 20 },
      "targets": [
        {
          "expr": "gospeak_tokens_created_total",
          "legendFormat": "tokens",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": {}, "overrides": [] }
    },
    {
      "type": "stat",
      "title": "Disconnects",
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 20 },
      "targets": [
        {
          "expr": "gospeak_disconnects_total",
          "legendFormat": "disconnects",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": {}, "overrides": [] }
    }
  ],
  "schemaVersion": 39,
  "tags": ["gospeak"],
  "templating": { "list": [] },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "GoSpeak Server",
  "uid": "gospeak-server",
  "version": 1
}
